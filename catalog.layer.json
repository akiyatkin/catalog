{
	"tpl":"*catalog/rubrics.tpl",
	"jsontpl":"*catalog/rubrics.php",
	"autoedittpl":{ "fast":"{infra.conf.catalog.dir}"},
	"unick":"catalog",
	"configtpl":{
		"root":"{crumb}"
	},
	"child":[{
		"tpl":"*catalog/search.tpl",
		"jsontpl":"*catalog/search.php?val={crumb.name}&m={infra.Crumb.get.m}&page={infra.Crumb.get.p}",
		"autoedittpl":{ "fast":"{infra.conf.catalog.dir}" },
		"conf_prod":true,
		"unick":"search",
		"configtpl":{
			"root":"{crumb.parent}"
		}
	},{
		"child":{
			"tpl":"*catalog/position.tpl",
			"autoedittpl":{ "fast":"*Каталог/{crumb.parent.name}/{crumb.name}/" },
			"jsontpl":"*catalog/position.php?val={crumb.parent.name}&art={crumb.name}",
			"seotpl":{"link":"{crumb.parent.parent}/{{producer}}/{{article}}"}
		}
	}],
	"childs":{
		"find":{
			"jsontpl":"*catalog/find.php",
			"configtpl":{
				"root":"{crumb.parent}"
			},
			"tpl":"*catalog/find.tpl",
			"autosavename":"catalog"
		},
		"producers":{
			"tpl":"*catalog/producers.tpl",
			"json":"*catalog/producers.php",
			"autoedit":{ "fast":"*Тексты/Производители"}
		},
		"stat":{
			"tpl":"*catalog/stat.tpl",
			"json":"*catalog/stat.php",
			"autoedit":{ "fast":"*Тексты/Статистика поиска по каталогу"},
			"seotpl":{
				"link":"{crumb}",
				"json":"{json}"
			}
		}
	}
}